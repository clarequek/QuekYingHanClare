{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "outDir": "./dist",
    "strict": true,
    "target": "ES5",
    "module": "CommonJS",
    "sourceMap": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
